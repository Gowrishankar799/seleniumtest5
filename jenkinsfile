pipeline {
    agent any
    
    tools {
       jdk 'jdk17'
       maven 'maven3'
    }

    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Gowrishankar799/seleniumtest4.git'
            }
        }
        
        stage('COMPILE') {
            steps {
                sh "mvn clean"
            }
        }
        
        stage('TEST') {
            steps {
                sh "mvn test"
            }
        }
    }
}

